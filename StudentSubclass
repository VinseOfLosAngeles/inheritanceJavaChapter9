public class Student extends Person
{
    private String major;
    public Student(String studentName, int studentBirthYear, String studentMajor)
    {
        Student.setName(studentName);
        Student.setBirthYear(studentBirthYear;
        major = studentMajor;
    }
    /**
    * @return returns a String with the student's information
    */
    public String toString()
    {
        return Student.getName + " " + Student.getBirthYear() + " " + studentMajor;
    }
}
